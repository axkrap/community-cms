<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE blocklist SYSTEM "../docs/blocks.dtd">
<blocklist>
	<block name="poll">
		<attribute name="question_id" label="Question ID" type="int">
		</attribute>
	</block>
	<block name="text">
		<attribute name="article_id" label="Article ID" type="int">
		</attribute>
		<attribute name="show_border" label="Show Border" type="option">
			<values>
				<value>yes</value>
				<value>no</value>
			</values>
		</attribute>
	</block>
	<block name="events">
		<attribute name="mode" label="Display Mode" type="option">
			<values>
				<value>upcoming</value>
				<value>past</value>
			</values>
		</attribute>
		<attribute name="num" label="Number of Entries" type="int">
		</attribute>
	</block>
	<block name="calendarcategories">
	</block>
</blocklist>
